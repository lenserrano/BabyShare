le: #245349;\n$color-subtitle:#3f7c6f;\n$color-linkhover: #54b67d;\n//  *   HEADER  *\n//Colors Backgrounds\n$bg-header: #b9ded6;\n$bg-footer: #b9ded6;\n$bg-signup: #c9f3db;\n$bg-signin: transparent;\n\n//Colors Fonts\n$color-signup: #54b67d;\n$color-signin: #2b6b48;\n\n//  *   PROMOCIONES  *\n//Colors Backgrounds\n$bg-promotions: #ecf5f0;\n\n//Colors Fonts\n$color-promotions: #24332c;\n\n//  *   VENDER  *\n//Colors Backgrounds\n$bg-selling: #ecf5f0;\n\n//Colors Fonts\n$color-selling: #146b41;\n\n//  *   FAVORITOS  *\n//Colors Backgrounds\n$bg-cards: #f3feed;\n\n//Colors Fonts\n$color-cards: #262922;\n\n//  *   FOOTER  *\n//Colors Backgrounds\n$bg-footer: #b9ded6;\n$bg-copy: #1c2522;\n\n//Colors Fonts\n$color-copy: #ffffff;\n\n\n//Fonts\n$font-title: 'Comfortaa', cursive;\n$font-normal: 'Assistant', sans-serif;\n\n// * SEARCH BAR *\n$border-search: 1px solid #f2fbf9;\n$r-search: 10px;\n$border-color: #20c5a1;\n$bg-search: url('../img/icons/search.png');\n\n//  *   DROPDOWN    *\n$bg-dropdown: lighten(#1c2522, .5);\n$color-dropdown: #e2f4f0;\n\n//  *   MARGIN  *\n$margin: 5px;\n\n//  * AVATAR *\n$border-avatar: 1px solid #f2fbf9;\n","assets/sass/_promotions.scss":".carousel{\n    margin-top: $margin;\n    margin-bottom: $margin;\n    @include flexbox(row, center, center);\n    img{\n        width: 80vw;\n        height: auto;\n    }\n}","assets/sass/_sell.scss":"#vender{\n    @include flexbox(row, center, center);\n    width: 80vw;\n    margin-top: $margin;\n    margin-bottom: $margin;\n}\n.container{\n    @include grid(1, 4);\n    width: 100%;\n}\nfigure{\n    @include flexbox(row, center, center);\n    flex-wrap: wrap;\n    margin: 0;\n    .process{\n        width: 10vw;\n        height: auto;\n    }\n    figcaption{\n        font-size: map-get($fs, small);\n        font-family: $font-normal;\n        color: $color-main;\n        font-weight: bold;\n    }\n}\n","assets/sass/_favorites.scss":"#favoritos{\n   \n    width: 80vw;\n    margin-top: $margin;\n    margin-bottom: $margin;\n}\n.title{\n    font-size: map-get($fs, normal);\n    font-family: $font-title;\n    font-weight: bold;\n    text-align: left !important;\n    color: $color-subtitle;\n    grid-area: title;\n}\n#mas-vendidos, .recomendados{\n    @include grid(3, 2);\n    width: 100%;\n    gap: $margin;\n}\n.card{\n    @include flexbox( row, center, center);\n    flex-wrap: wrap;\n}\n.favorites{\n    width: 100%;\n    img{\n        width: 35vw;\n        height: auto;\n    }\n    .details{\n        /* @include flexbox(row, center, center);\n        flex-wrap: nowrap; */\n        width: 35vw;\n        font-size: map-get($fs, small );\n        font-family: $font-normal;\n        font-weight: 300;\n        color: $color-main;\n        \n    }\n}\n.price-like{\n    display: inherit;\n    justify-content: space-evenly;\n    align-items: center;\n    width: 100%;\n    .precio{\n        font-size: map-get($fs, normal );\n        font-weight: bold;\n        padding: .5em 2em .2em .5em;\n    }\n}\n.name{\n    display: inherit;\n    justify-content: space-evenly;\n    align-items: center;\n    width:100%;\n    padding: .2em 2em .5em .5em;\n}\n\n.seller-details{\n    display: inherit;\n    align-items: center;\n    margin: 0;\n    .avatar{\n            width: 15px;\n            height: 15px;\n            border: $border-avatar;\n            border-radius: 50%;\n            padding-right: 5px;\n    }\n    .seller{\n        font-size: map-get($fs, small);\n        font-family: $font-normal;\n        color: $color-main;\n    }\n}","assets/sass/_footer.scss":"footer{\n    width: 100vw;\n    height: auto;\n    font-family: $font-normal;\n    background-color: $bg-footer;\n    color: $color-main;\n    \n}\n.mobile-footer{\n    @include flexbox( row, space-between, center );\n    margin: 1% 10%;\n}\n.icons-footer{\n    width: 40px;\n    height: auto;\n}\n.main-icon-footer{\n    border: 1px solid white;\n    border-radius: 50%;\n    background: white;\n    width: 60px;\n    height: 60px